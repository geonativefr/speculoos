root = true

[*]
indent_size = 4
charset = utf-8
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.scss]
indent_size = 2

[*.js]
indent_size = 2

[*.vue]
indent_size = 2

[*.yml]
indent_size = 2

[*.json]
indent_size = 2
